{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "lib": ["ES2022"],
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "composite": true,
    "exactOptionalPropertyTypes": false,
    "allowUnusedLabels": true,
    "checkJs": false,
    "strict": true
  }
}
